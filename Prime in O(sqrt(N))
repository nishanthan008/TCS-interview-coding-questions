#include <iostream>
#include<cmath>
using namespace std;
int main()
{
    int n=8,flag=0;
    cout<<"enter your number:";
    cin>>n;
    for(int i=2;i<=sqrt(n);i++)
    {
        if(n%i==0)
        {
            flag=1;
        }
    }
    if(flag==1)
    {
    cout<<"not a prime";
    }
    else
    {
     cout<<"Prime";   
    }
}
