#include <iostream>
using namespace std;
int main()
{
    int n,sum=0;
    cout<<"enter your number:";
    cin>>n;
    if(n<5)
    {
        cout<<"0";
    }
    else
    {
        while(n>=5)
        {
            sum+=n/5;
            n=n/5;
        }
        cout<<"No.of Zeros: "<<sum;
    }
    return 0;
}
