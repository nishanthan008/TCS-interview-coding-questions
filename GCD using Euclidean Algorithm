#include <iostream>
using namespace std;
int gcd(int a,int b)
{
    if(b==0)
    {
        return a;
    }
    else
    {
        return gcd(b,a%b);
    }
}
int main()
{
    int a,b;
    cout<<"enter your number1:";
    cin>>a;
    cout<<"enter your number1:";
    cin>>b;
    int res=gcd(a,b);
    cout<<"gcd:"<<res;  
}
